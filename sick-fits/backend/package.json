{
  "name": "sick-fits-backend",
  "version": "2.0.0",
  "private": true,
  "author": "Wes Bos",
  "license": "MIT",
  "scripts": {
    "dev": "keystone-next",
    "seed-data": "keystone-next --seed-data"
  },
  "eslintConfig": {
    "extends": "wesbos/typescript.js",
    "rules": {
      "@typescript-eslint/no-unsafe-assignment": 0
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 10,
            "browsers": [
              "last 2 chrome versions",
              "last 2 firefox versions",
              "last 2 safari versions",
              "last 2 edge versions"
            ]
          }
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "dependencies": {
    "@keystone-6/auth": "^8.0.0",
    "@keystone-6/cloudinary": "^8.0.0",
    "@keystone-6/core": "^6.3.0",
    "@keystone-6/document-renderer": "^1.1.2",
    "@keystone-6/fields-document": "^9.1.0",
    "@types/nodemailer": "^6.4.0",
    "dotenv": "^16.2.0",
    "next": "^14.0.5",
    "nodemailer": "^6.4.17",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "stripe": "^16.9.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^9.0.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "typescript": "^5.1.2"
  },
  "engines": {
    "node": ">=21.0.0"
  },
  "repository": "https://github.com/wesbos/advanced-react"
}
